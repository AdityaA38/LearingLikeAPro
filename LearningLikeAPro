import SwiftUI

struct ContentView: View {
    var body: some View {
        
                TabView {
                    NavigationView{
                        VStack{
                            Image("TheABC's")
                                .resizable()
                                .frame(width:250, height:250)
                            .navigationTitle("This is the Alphabet")
                                
                         NavigationLink(destination:LetterA(),label: {
                                Text("Next Alphabet")
                                    .bold()
                                    .frame(width:280, height: 50)
                                    .background(Color.black)
                                    .foregroundColor(.white)
                                    .cornerRadius(/*@START_MENU_TOKEN@*/3.0/*@END_MENU_TOKEN@*/)
                            })
                        }
                        
                    }
                        .tabItem {
                            Image(systemName:"1.square.fill")
                            Text("Alphabet")
                        }
                   
                    NavigationView{
                        VStack{
                            Image("numbers")
                                .resizable()
                                .frame(width:250, height:250)
                            .navigationTitle("Numbers")
                                
                         NavigationLink(destination:Numbers(),label: {
                                Text("Start learning your numbers")
                                    .bold()
                                    .frame(width:280, height: 50)
                                    .background(Color.black)
                                    .foregroundColor(.white)
                                    .cornerRadius(/*@START_MENU_TOKEN@*/3.0/*@END_MENU_TOKEN@*/)
                            })
                        }
                        
                    }
                    
                        .tabItem {
                            Image(systemName: "2.square.fill")
                            Text("Shapes")
                        }
                    NavigationView{
                        VStack{
                            Image("Shapes")
                                .resizable()
                                .frame(width:250, height:250)
                            .navigationTitle("All About Shapes")
                                
                         NavigationLink(destination:Square(),label: {
                                Text("Start learning about shapes")
                                    .bold()
                                    .frame(width:280, height: 50)
                                    .background(Color.black)
                                    .foregroundColor(.white)
                                    .cornerRadius(/*@START_MENU_TOKEN@*/3.0/*@END_MENU_TOKEN@*/)
                            })
                        }
                        
                    }
                        .tabItem {
                           Image(systemName: "3.square.fill")
                            Text("Shapes")
                        }
                }
                .font(.headline)
         

                    
    }
}
