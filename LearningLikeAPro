struct Numbers: View{
    var body: some View{
        ScrollView {

        VStack{
            Text("Here are the Numbers 1 throught 10")
            Image("zero")
                .resizable()
            .frame(width:100, height:100)
            Spacer()
                .frame(height:20)
                Text("Zero")
            Text("Zero is technically not the first number")
            Text("It is still a whole number")
            Text("The number zero comes right before 1.")
        }
        VStack{
            Spacer()
                .frame(height:20)
            Image("one")
                .resizable()
                .frame(width:100, height:100)
            Spacer()
                .frame(height:20)
              Text("One")
            Text("One is the first digit or number.")
            Text("It comes right after the number zero.")
            
        }
    VStack{
            Spacer()
                .frame(height:20)
        Image("two")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
          Text("Two")
        Text("Two is the second digit or number.")
        Text("It comes right after the number one.")
            
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("three")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
          Text("Three")
            Text("Three is the third digit or number.")
            Text("It comes right after the number two.")
            
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("four")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
            Text("Four")
            Text("Four is the fourth digit or number.")
            Text("It comes right after the number five.")
            
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("five")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
            Text("Five")
            Text("Five is the fifth digit or number.")
            Text("It comes right after the number four.")
            
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("six")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
            Text("Six")
        Text("Six is the sixth digit or number.")
        Text("It comes right after the number five.")
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("seven")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
         Text("Seven")
            Text("Seven is the seventh digit or number.")
            Text("It comes right after the number six.")
            
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("eight")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
         Text("Eight")
            Text("Eight is the eigth digit or number.")
            Text("It comes right after the number seven.")
            
        }
        VStack{
            Spacer()
                .frame(height:20)
        Image("nine")
            .resizable()
            .frame(width:100, height:100)
        Spacer()
            .frame(height:20)
         Text("Nine")
            Text("Nine is the nineth digit or number.")
            Text("It comes right after the number eight.")
            
        }
    }
    }// End of scroll view
}
    

struct LetterA: View {
    var body: some View {
       
